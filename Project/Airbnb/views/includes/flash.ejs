
 <%   if(success && success.length>0){ %>
    <div class="alert my-3  col-6 offset-3 alert-success alert-dismissible" role="alert">
       <%= success %>
       <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
      </div>

<% }  %>

<%   if(error && error.length>0){ %>
    <div class="alert my-3  col-6 offset-3 alert-danger alert-dismissible" role="alert">
       <%= error %>
       <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
      </div>

<% }  %>



